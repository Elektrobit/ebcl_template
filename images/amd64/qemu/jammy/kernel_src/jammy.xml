<ns0:RootFileSystem xmlns:ns0="https://www.linutronix.de/projects/Elbe" xmlns:xi="http://www.w3.org/2003/XInclude" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" created="2009-05-20T08:50:56" revision="6" xsi:schemaLocation="https://www.linutronix.de/projects/Elbe dbsfed.xsd">
	<project>
		<name>jammy</name>
		<version>1.0</version>
		<description>Ubuntu Jammy test image.</description>
		<buildtype>amd64</buildtype>
		<mirror>
			<primary_host>archive.ubuntu.com</primary_host>
			<primary_path>/ubuntu</primary_path>
			<primary_proto>http</primary_proto>
			<url-list>
				<url>
					<binary>http://archive.ubuntu.com/ubuntu jammy main universe</binary>
					<source>http://archive.ubuntu.com/ubuntu jammy main universe</source>
					<options>
						<option>arch=amd64</option>
					</options>
				</url>
				<url>
					<binary>http://archive.ubuntu.com/ubuntu jammy-security main universe</binary>
					<source>http://archive.ubuntu.com/ubuntu jammy-security main universe</source>
					<options>
						<option>arch=amd64</option>
					</options>
				</url>
			</url-list>
		</mirror>
		<suite>jammy</suite>
	</project>
	<target>
		<hostname>jammy</hostname>
		<domain>elektrobit.com</domain>
		<passwd>linux</passwd>
		<console>ttyAMA0,115200</console>
		<debootstrap>
			<variant>minbase</variant>
		</debootstrap>
		<package>
			<tar>
				<name>root.tar</name>
			</tar>
		</package>
		<pkg-list>
			<pkg>systemd</pkg>
			<pkg>udev</pkg>
			<pkg>util-linux</pkg>
		</pkg-list>
		<finetuning>
			<!-- use systemd as init manager -->
			<command>ln -s /usr/lib/systemd/systemd /sbin/init</command>
		</finetuning>
		<project-finetuning>
			<set_packer packer="none">root.tar</set_packer>
		</project-finetuning>
	</target>
</ns0:RootFileSystem>
