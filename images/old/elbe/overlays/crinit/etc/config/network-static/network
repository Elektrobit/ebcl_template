# !LINKSTO ireq.config.ipv6, 1
# !LINKSTO ireq.config.ipv4, 1
config device
    option name     lo
    option ipv6     0

config interface loopback
    option device	lo
    option proto	static
    option ipaddr	127.0.0.1
    option netmask	255.0.0.0

config device
    option name     enp0s1

config interface lan
    option device       enp0s1
    option proto        static
    option ipaddr       192.168.1.11/24
    option gateway      192.168.1.1
    option ip6addr      fd00::eb:2/64
    option ip6gateway   fe80::1
